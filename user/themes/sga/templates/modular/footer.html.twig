<section class="container mx-auto px-8 my-8">
	<img class="w-full mb-8" src="{{ header.image|first.path }}" />
	<div class="flex flex-wrap text-center">
		{% for column in header.columns %}
			<div class="md:w-1/3 w-full">
				<p class="text-xl font-bold text-secondary mb-4">
					{{ column.title }}
				</p>
				<div>
					{% for item in column.items %}
						<a class="block mb-2" href="{{ item.url }}">{{ item.title }}</a>
					{% endfor %}
				</div>
			</div>
		{% endfor %}
		<div class="md:w-1/3 w-full text-center">
			<p class="markdown leading-7 mb-4">
				{{ header.credits }}
			</p>
			<p class="leading-7 mb-4">
				Website developed by
				<span class="font-bold">Akash Bhave (2021)</span>
				and
				<span class="font-bold">Michael Fatemi (2022)</span>, inspired by the previous website developed by
				<span class="font-bold">Anubis Watal (2019)</span>.
			</p>
			<p>
				Â© 2018-{{ 'now'|date('Y') }} TJHSST SGA
			</p>
			<p>
				6560 Braddock Road, Alexandria, VA 22312.
			</p>
		</div>
	</div>
</section>
